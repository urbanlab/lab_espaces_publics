{
  "apiVersion": 3,
  "name": "labeps-theme/carousel",
  "title": "Carousel",
  "category": "design",
  "icon": "images-alt",
  "description": "A carousel block that supports images, posts, and custom post types.",
  "supports": {
    "html": false,
    "align": true
  },
  "attributes": {
    "images": {
      "type": "array",
      "default": []
    },
    "columns": {
      "type": "number",
      "default": 1
    },
    "contentType": {
      "type": "string",
      "default": "images",
      "enum": ["images", "posts", "inspirations", "projects", "ressources"]
    },
    "posts": {
      "type": "array",
      "default": []
    },
    "postSelections": {
      "type": "array",
      "default": []
    },
    "selectedPostId": {
      "type": "number",
      "default": 0
    },
    "categories": {
      "type": "array",
      "default": []
    }
  },
  "editorScript": "theme-carousel-block",
  "style": "theme-carousel-block"
}
